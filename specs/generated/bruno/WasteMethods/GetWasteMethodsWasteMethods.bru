meta {
  name: GetWasteMethodsWasteMethods
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/wastemethods/v2
  body: json
  auth: basic
}

auth:basic {
  username: {{username}}
  password: {{password}}
}

docs {
