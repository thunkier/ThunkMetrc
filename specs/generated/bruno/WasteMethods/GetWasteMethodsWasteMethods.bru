    {
      "Name": "Grinder",
      "ForPlants": true,
      "ForProductDestruction": false,
      "LastModified": "0001-01-01T00:00:00+00:00"
    },
    {
      "Name": "Compost",
      "ForPlants": true,
      "ForProductDestruction": false,
      "LastModified": "0001-01-01T00:00:00+00:00"
    },
    {
      "Name": "Incinerator",
      "ForPlants": false,
      "ForProductDestruction": true,
      "LastModified": "0001-01-01T00:00:00+00:00"
    }
  ]
}
