meta {
  name: GetCounties
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/sales/v2/counties
  body: json
  auth: basic
}

auth:basic {
  username: {{username}}
  password: {{password}}
}

docs {
  Returns a list of counties available for sales deliveries.

  Example Response:
  {
    "Data": [
      {
        "Id": 1,
        "Name": "Los Angeles County"
      },
      {
        "Id": 2,
        "Name": "Sacramento County"
      }
    ],
    "Total": 2,
    "TotalRecords": 2,
    "PageSize": 2,
    "RecordsOnPage": 2,
    "Page": 1,
    "CurrentPage": 1,
    "TotalPages": 1
  }
}
