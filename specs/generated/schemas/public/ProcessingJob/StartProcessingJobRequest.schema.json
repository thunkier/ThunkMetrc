  "properties": {
    "CountUnitOfMeasure": {
      "type": "string"
    },
    "JobName": {
      "type": "string"
    },
    "JobType": {
      "type": "string"
    },
    "Packages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Label": {
            "type": "string"
          },
          "Quantity": {
            "type": "number"
          },
          "UnitOfMeasure": {
            "type": "string"
          }
        },
        "required": [
          "Label",
          "Quantity",
          "UnitOfMeasure"
        ]
      }
    },
    "StartDate": {
      "type": "string",
      "format": "date-time"
    },
    "VolumeUnitOfMeasure": {
      "type": "string"
    },
    "WeightUnitOfMeasure": {
      "type": "string"
    }
  },
  "required": [
    "CountUnitOfMeasure",
    "JobName",
    "JobType",
    "Packages",
    "StartDate",
    "VolumeUnitOfMeasure",
    "WeightUnitOfMeasure"
  ]
}