{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Employee",
  "type": "object",
  "properties": {
    "FullName": {
      "type": "string"
    },
    "IsIndustryAdmin": {
      "type": "boolean"
    },
    "IsManager": {
      "type": "boolean"
    },
    "IsOwner": {
      "type": "boolean"
    },
    "License": {
      "type": "object",
      "properties": {
        "EmployeeLicenseNumber": {
          "type": "string"
        },
        "EndDate": {
          "type": "string",
          "format": "date"
        },
        "LicenseType": {
          "type": "string"
        },
        "Number": {
          "type": "string"
        },
        "StartDate": {
          "type": "string",
          "format": "date"
        }
      },
      "required": [
        "EmployeeLicenseNumber",
        "EndDate",
        "LicenseType",
        "Number",
        "StartDate"
      ]
    }
  },
  "required": [
    "FullName",
    "IsIndustryAdmin",
    "IsManager",
    "IsOwner",
    "License"
  ]
}