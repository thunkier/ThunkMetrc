meta {
  name: GetByCaregiverLicenseNumber V2
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/caregivers/v2/status/{caregiverLicenseNumber}
  body: json
  auth: basic
}

params:query {
  licenseNumber: 
}

auth:basic {
  username: {{username}}
  password: {{password}}
}

docs {
  Retrieves the status of a Caregiver by their License Number for a specified Facility. Data returned by this endpoint is cached for up to one minute.

  Permissions Required:
  - Lookup Caregivers

  Example Response:
  {
    "CaregiverLicenseNumber": "CGN-123-456",
    "Active": true,
    "Patients": [
      "PatientLicense_74d59",
      "PatientLicense_b993f"
    ]
  }
}
